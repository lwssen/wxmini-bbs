<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sss.sssforum.wxclient.file.dao.IFileInfoDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sss.sssforum.wxclient.file.entity.FileInfo">
        <id column="id" property="id" />
        <result column="file_name" property="fileName" />
        <result column="save_path" property="savePath" />
        <result column="request_url" property="requestUrl" />
        <result column="size" property="size" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, file_name, save_path, request_url, size, create_time, update_time
    </sql>

</mapper>
